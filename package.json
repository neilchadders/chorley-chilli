{
    "name": "Chorley-Chilli",
    "version": "2.0.0",
    "description": "\"eCommerce full stack\"",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node back-end/server.js",
        "server": "nodemon back-end/server.js",
        "client": "npm start --prefix front-end",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node back-end/seeder.js",
        "data:destroy": "node back-end/seeder.js -d",
        "build": "npm install && npm install --prefix front-end && npm run build --prefix front-end"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@paypal/react-paypal-js": "^8.3.0",
        "@reduxjs/toolkit": "^2.2.3",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.3.3",
        "multer": "^1.4.5-lts.1",
        "react-redux": "^9.1.2"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.1.0"
    }
}
